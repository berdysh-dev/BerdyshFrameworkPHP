{
    "name": "berdysh-dev/berdysh-framework",
    "type": "library",
    "description": "The skeleton application for the Berdysh framework.",
    "keywords": ["framework","berdysh"],
    "homepage": "http://berdysh.net/",
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "berdysh-dev",
            "email": "berdysh.dev@gmail.com"
        }
    ],
    "prefer-stable": true,
    "require": {
        "php": "^5.6|^7.0|^8.0",
        "guzzlehttp/guzzle": "^7.2",
        "google/cloud-core": "*",
        "google/cloud-logging": "*",
        "google/auth": "*"
    },
    "require-dev": {
        "php": "^5.6|^7.0|^8.0",
        "phpunit/phpunit": "*"
    },
    "suggest": {
      "ext-protobuf": "For better performance, install the protobuf C extension.",
      "google/protobuf": "To get started using grpc quickly, install the native protobuf library."
    },
    "autoload": {
        "classmap": [
            "src/"
        ],
        "files": [
            "src/BerdyshFramework.php"
        ]
    },
    "autoload-dev": {
        "classmap": [
            "src/"
        ],
        "files": [
            "src/BerdyshFramework.php"
        ]
    }
}
